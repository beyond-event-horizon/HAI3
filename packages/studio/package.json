{
  "name": "@hai3/studio",
  "version": "0.1.0",
  "description": "HAI3 Studio - Development studio overlay for HAI3 applications",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": [
    "./src/i18n/index.ts",
    "./dist/i18n/index.js",
    "./dist/i18n/index.mjs"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch"
  },
  "keywords": [
    "hai3",
    "studio",
    "development",
    "react"
  ],
  "author": "HAI3 Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/beyond-event-horizon/HAI3.git",
    "directory": "packages/studio"
  },
  "dependencies": {
    "lodash": "^4.17.21"
  },
  "peerDependencies": {
    "@hai3/uikit": "^0.1.0",
    "@hai3/uikit-contracts": "^0.1.0",
    "@hai3/uicore": "^0.1.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.20",
    "@types/node": "^24.9.1",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "tsup": "^8.5.0",
    "typescript": "^5.4.2"
  }
}
