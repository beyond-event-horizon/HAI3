{
  "name": "@hai3/uicore",
  "version": "0.1.0-alpha.0",
  "description": "HAI3 UI Core - Layout system and Redux architecture for SaaS control panels",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": [
    "src/api/services/*Service.ts",
    "dist/api/services/*Service.js",
    "dist/api/services/*Service.mjs"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch"
  },
  "keywords": [
    "hai3",
    "react",
    "redux",
    "layout",
    "ui-core",
    "saas"
  ],
  "author": "HAI3org",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/HAI3org/HAI3.git",
    "directory": "packages/uicore"
  },
  "bugs": {
    "url": "https://github.com/HAI3org/HAI3/issues"
  },
  "homepage": "https://github.com/HAI3org/HAI3#readme",
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@hai3/uikit-contracts": "*",
    "@reduxjs/toolkit": "^2.2.1",
    "axios": "^1.13.1",
    "lodash": "^4.17.21",
    "react-redux": "^9.1.0",
    "react-router-dom": "^6.28.0"
  },
  "peerDependencies": {
    "@hai3/studio": "*",
    "@hai3/uikit-contracts": "*",
    "@reduxjs/toolkit": "^2.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "peerDependenciesMeta": {
    "@hai3/studio": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/lodash": "^4.17.20",
    "@types/node": "^24.9.1",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "tsup": "^8.5.0",
    "typescript": "^5.4.2"
  }
}
